{
  "containerDefinitions": [
    {
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 9090,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "name": "test-container",
      "image": "nginx",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "ecs-log-streaming",
          "awslogs-region": "us-east-2"
        }
      },
      "cpu": 0
    }
  ],
  "family": "test"
}
